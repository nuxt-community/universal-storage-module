    "@nuxt/typescript-build": "^3.0.2",
    "@nuxtjs/eslint-config-typescript": "latest",
    "@types/cookie": "latest",
    "codecov": "latest",
    "eslint": "latest",
    "jest": "latest",
    "jsdom": "latest",
    "mkdist": "latest",
    "nuxt-edge": "latest",
    "siroc": "latest",
    "standard-version": "latest"
  },
  "publishConfig": {
