{
  "name": "@nuxtjs/universal-storage",
  "version": "0.5.9",
  "description": "Universal Storage Utilities for Nuxt",
  "repository": "nuxt-community/universal-storage-module",
  "license": "MIT",
  "main": "dist/module.js",
  "types": "dist/module.d.ts",
